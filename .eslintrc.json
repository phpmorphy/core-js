{
    "env": {
        "browser": true,
        "es6": true,
        "mocha": true
    },
    "globals": {
        "assert": true,
        "umi": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "impliedStrict": true
    },
    "plugins": [
        "@typescript-eslint"
    ]
}
