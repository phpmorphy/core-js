{
  "name": "@umi-top/umi-core-js",
  "version": "1.0.0",
  "description": "UMI Core JS Library",
  "type": "commonjs",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "es2015": "esm/index.js",
  "types": "esm/index.d.ts",
  "unpkg": "iife/index.min.js",
  "sideEffects": false,
  "scripts": {
    "pretest": "tsc --target es2015 --module commonjs --removeComments --outDir dist ./src/index.ts",
    "test": "nyc -r text -r html mocha --recursive",
    "posttest": "rm -rf dist",
    "cleanup": "rm -rf ./tmp ./dist ./esm ./cjs ./iife ./doc ./coverage ./.nyc_output",
    "prepack": "npm run build; echo '{\"type\":\"module\",\"types\": \"index.d.ts\"}' > ./esm/package.json; echo '{\"type\":\"commonjs\",\"types\": \"../esm/index.d.ts\"}' > ./cjs/package.json;  echo '{\"types\": \"../esm/index.d.ts\"}' > ./cjs/package.json;",
    "build": "npm run cleanup; mkdir -p tmp; npm run build:concat; npm run build:compile; npm run build:rollup",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "eslint": "eslint src/**/*.ts",
    "jsdoc": "jsdoc --pedantic -d ./doc ./esm/index.js",
    "build:concat": "concat ./src/**/*.ts ./src/**/**/*.ts -o ./tmp/index.ts; sed -i .tmp -e '/^import/d' ./tmp/index.ts",
    "build:compile": "tsc --emitDeclarationOnly --declaration --declarationDir esm; tsc",
    "build:rollup": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/umi-top/umi-core-js"
  },
  "keywords": [
    "UMI",
    "umi.top"
  ],
  "author": "UMI <dev@umi.top>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umi-top/umi-core-js/issues"
  },
  "homepage": "https://umi.top",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "chai": "^4.2.0",
    "concat": "^1.0.3",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "rollup": "^2.16.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "esm",
    "cjs",
    "iife"
  ]
}
