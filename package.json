  "engines": {
    "node": ">=4"
  },
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "test": "nyc --reporter=text --reporter=html mocha --recursive",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "jsdoc --pedantic -c jsdoc.json",
    "lint": "eslint src/**/*.ts dist/**/*.js lib/index.js lib/index.mjs; remark ."
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/umi-top/umi-core-js"
  },
  "keywords": [
    "UMI",
    "umi.top"
  ],
  "bugs": {
    "url": "https://github.com/umi-top/umi-core-js/issues"
  },
  "homepage": "https://umi.top",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.0.19",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "better-docs": "^2.3.0",
    "chai": "^4",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "flowgen": "^1.11.0",
    "jsdoc": "^3.6.4",
    "mocha": "^8",
    "nyc": "^15",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.1",
    "remark-preset-lint-markdown-style-guide": "^3.0.1",
    "rollup": "^2.21.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "standard": "^14.3.4",
